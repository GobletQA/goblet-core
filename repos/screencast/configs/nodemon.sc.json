{
  "delay": "500",
  "exec": "yarn build && yarn start",
  "ext": "ts,mjs,js,json,graphql",
  "watch": [
    "src",
    "config",
    "../shared"
  ],
  "ignoreRoot": [
    ".git",
    "docs",
    "node_modules/keg-core",
    "node_modules/.bin",
    "scripts",
    "tasks",
    "tests",
    "temp"
  ]
}
