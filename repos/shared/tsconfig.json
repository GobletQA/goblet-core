{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "paths": {
      "@GSH/Template": ["src/templates"],
      "@GSH/FileSys": ["src/libs/fileSys"],
      "@GSH/Features": ["src/libs/features"],
      "@GSH/Definitions": ["src/libs/definitions"],
      "@GSH/Libs": ["src/libs"],
      "@GSH/Utils": ["src/utils"],
      "@GSH/Exp": ["src/express"],
      "@GSH/Models": ["src/models"],
      "@GSH/App": ["src/express/app.js"],
      "@GSH/Paths": ["src/utils/paths.js"],
      "@GSH/Middleware": ["src/middleware"],
      "@GSH/Router": ["src/express/appRouter.js"],
      "@GSH/Config": ["src/utils/getGobletConfig.js"],
      "@GSH/AsyncWrap": ["src/express/asyncWrap.js"],
      "@GSH/Repo": ["src/repo"],
      "@GSH": ["src"],
      "@GSHRoot": ["."],
      "@GEnv": ["../shared/src/environment/environment"],
      "@GEnv/secrets": ["../shared/src/environment/secrets"],
      "@GEnv/values": ["../shared/src/environment/values"],
      "@GEnvironment": ["../shared/src/environment/environment"],
      "@GEnvironment/secrets": ["../shared/src/environment/secrets"],
      "@GEnvironment/values": ["../shared/src/environment/values"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
