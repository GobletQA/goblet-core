{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "paths": {
      "@GCD": ["src"],
      "@GCDRoot": ["./"],
      "@GCD/Proxy": ["src/proxy"],
      "@GCD/Utils": ["src/utils"],
      "@GCD/Configs": ["configs"],
      "@GCD/Docker": ["src/docker"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}