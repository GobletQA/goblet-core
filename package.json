{
  "name": "@keg-hub/keg-herkin",
  "version": "1.3.3",
  "description": "Web application testing framework built on the Keg Platform",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "url": "git+https://github.com/keg-hub/keg-herkin.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/keg-hub/keg-herkin/issues"
  },
  "author": "Lance Tipton <lancetipton04@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "api": "yarn api:back",
    "api:back": "nodemon --config configs/nodemon.backend.json",
    "api:sc": "yarn sc:api",
    "build": "exit 0",
    "build:fe": "node ./tasks/runTask.js deploy build -- frontend=true",
    "bundle": "node ./scripts/bundle.js",
    "clean": "cd node_modules/keg-core; yarn clean",
    "clean:install": "yarn clean:full; yarn install",
    "clean:full": "yarn clean; yarn clean:nm; yarn install",
    "clean:nm": "rimraf ./yarn.lock; rimraf ./package.lock; rimraf node_modules",
    "clean:rn": "yarn keg clean:rn",
    "dev": "run-p web api:back",
    "ex": "yarn ex:dev",
    "ex:build": "cd ./repos/example && yarn build",
    "ex:dev": "cd ./repos/example && yarn dev",
    "ex:i": "yarn ex:install",
    "ex:install": "cd ./repos/example && yarn install",
    "ex:start": "yarn ex:dev",
    "format": "yarn pretty",
    "ios": "cd node_modules/keg-core; yarn ios",
    "keg": "cd node_modules/keg-core; yarn",
    "pretty": "./node_modules/.bin/prettier --config ./configs/prettier.config.js --write '**/*.{js,jsx}'",
    "setup": "yarn install --ignore-engines; cd node_modules/keg-core; yarn postinstall --ignore-engines",
    "start": "cd node_modules/keg-core; expo start",
    "sc": "PWDEBUG=1 DEBUG=pw:api node ./repos/screencast/src/screencast.js",
    "sc:api": "nodemon --config configs/nodemon.screencast.json",
    "sc:d": "yarn sc:daemon",
    "sc:daemon": "PWDEBUG=1 DEBUG=pw:api node ./repos/screencast/src/screencast.js --daemon",
    "sc:k": "yarn sc:kill",
    "sc:kill": "node ./repos/screencast/src/screencast.js --kill-all-screencast",
    "sc:r": "yarn sc:restart",
    "sc:restart": "node ./repos/screencast/src/screencast.js --restart",
    "sc:s": "yarn sc:status",
    "sc:status": "PWDEBUG=1 DEBUG=pw:api node ./repos/screencast/src/screencast.js --status",
    "test:e2e": "NODE_ENV=test jest --no-cache --config=./configs/jest.waypoint.config.js",
    "task": "node ./tasks/runTask.js",
    "web": "yarn web:dev",
    "web:build": "yarn web:bundle && yarn web:copy:bundle",
    "web:bundle": "cd node_modules/keg-core; yarn build:web && cd ../../",
    "web:copy:bundle": "rm -rf ./bundle/tap && cp -R ./node_modules/keg-core/web-build ./bundle/tap",
    "web:dev": "cd node_modules/keg-core && yarn web",
    "wf:t": "yarn workflow:test",
    "workflow:test": "node repos/workflows/tests/e2e/e2e.js"
  },
  "dependencies": {
    "@babel/core": "7.16.7",
    "@babel/preset-env": "7.16.7",
    "@keg-hub/args-parse": "8.0.0",
    "@keg-hub/ask-it": "1.1.0",
    "@keg-hub/cli-utils": "0.6.0",
    "@keg-hub/jsutils": "9.0.3",
    "@keg-hub/parse-config": "2.0.0",
    "@keg-hub/spawn-cmd": "0.3.0",
    "@ltipton/parkin": "1.1.1",
    "@ltipton/sockr": "0.3.0",
    "app-root-path": "3.0.0",
    "axios": "0.24.0",
    "babel-jest": "26.6.3",
    "cookie-session": "2.0.0",
    "core-js": "3.20.2",
    "cors": "2.8.5",
    "express": "4.17.3",
    "express-blacklist": "1.0.3",
    "express-defend": "1.0.9",
    "express-jwt": "6.1.1",
    "express-winston": "4.2.0",
    "http-proxy-middleware": "2.0.4",
    "is-secret": "1.2.1",
    "jest": "26.4.2",
    "jest-html-reporter": "3.4.2",
    "jest-playwright-preset": "1.7.0",
    "jsonwebtoken": "8.5.1",
    "mime": "3.0.0",
    "module-alias": "2.2.2",
    "nodemon": "2.0.13",
    "playwright": "1.22.2",
    "vm2": "3.9.9",
    "winston": "3.6.0"
  },
  "resolutions": {
    "@keg-hub/args-parse": "8.0.0",
    "@keg-hub/ask-it": "^1.1.0",
    "@keg-hub/cli-utils": "0.6.0",
    "@keg-hub/jsutils": "9.0.3",
    "@monaco-editor/react": "4.3.1"
  },
  "devDependencies": {
    "@cucumber/language-service": "0.11.0",
    "@monaco-editor/react": "4.3.1",
    "@novnc/novnc": "1.3.0",
    "firebase": "9.6.9",
    "keg-core": "npm:@keg-hub/keg-core@10.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "semver": "7.3.5"
  }
}
